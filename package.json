{
  "name": "regulondb-graphql-api",
  "version": "1.0.0",
  "engines": {
    "node": "16.13.0"
  },
  "description": "GraphQL web services for RegulonDB",
  "main": "gateway.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && npm run services",
    "services": "concurrently -k npm:service:*",
    "service:closedTools": "node build/closedServices/closedServicesServer.js",
    "service:openTools": "node build/openServices/openServicesServer.js",
    "service:gateway": "node build/gateway.js",
    "build": "babel src -d build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regulondbunam/GraphQL-api.git"
  },
  "contributors": [
    "Andrés Gerardo López Almazo",
    "Daniel Moreno Alonso",
    "Edgar Enrique Hernández Marcelo"
  ],
  "license": "ISC",
  "dependencies": {
    "@apollo/federation": "^0.33.9",
    "@apollo/gateway": "^0.46.0",
    "@graphql-tools/load-files": "^6.5.2",
    "@graphql-tools/merge": "^8.2.1",
    "apollo-fetch": "^0.6.0",
    "apollo-server-express": "^3.6.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^14.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.2.0",
    "graphql": "^15.5.0",
    "mongodb-filter-object-parser": "^1.3.2",
    "mongoose": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "axios": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "braces": "^3.0.2",
    "concurrently": "^7.0.0",
    "handlebars": "^4.7.7",
    "jest": "^27.4.7",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.5.1"
  }
}
