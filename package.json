{
  "name": "regulondb-graphql-api",
  "version": "1.0.0",
  "engines": {
    "node": "16.13.0"
  },
  "description": "GraphQL web services for RegulonDB",
  "main": "gateway.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && npm run services",
    "services": "concurrently -k npm:service:*",
    "service:closedTools": "node build/closedServices/closedServicesServer.js",
    "service:openTools": "node build/openServices/openServicesServer.js",
    "service:gateway": "node build/gateway.js",
    "build": "babel src -d build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regulondbunam/GraphQL-api.git"
  },
  "contributors": [
    "Andrés Gerardo López Almazo",
    "Daniel Moreno Alonso",
    "Edgar Enrique Hernández Marcelo"
  ],
  "license": "ISC",
  "dependencies": {
    "@apollo/federation": "^0.20.7",
    "@apollo/gateway": "^0.21.4",
    "@graphql-tools/load-files": "^6.5.2",
    "@graphql-tools/merge": "^8.2.1",
    "apollo-fetch": "^0.7.0",
    "apollo-server-express": "^2.25.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "graphql": "^15.5.0",
    "mongodb-filter-object-parser": "^1.2.6",
    "mongoose": "^5.11.15"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "axios": "^0.21.4",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^25.5.1",
    "braces": "^3.0.2",
    "concurrently": "^6.4.0",
    "handlebars": "^4.7.7",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1"
  }
}
